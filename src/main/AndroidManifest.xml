        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.A2_MovieList"
            />
        <activity android:name=".activities.CustomAdapter" />
        <activity android:name=".dataModel.Movie" />
        <activity android:name=".dataModel.MovieManager" />
        <activity android:name=".dataModel.Actor" />
        <activity android:name=".dataModel.Genre" />
        <activity android:name=".activities.A1_Splash"
            android:launchMode="singleInstance"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

