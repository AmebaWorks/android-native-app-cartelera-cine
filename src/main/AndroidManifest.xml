        <activity android:name=".Constants" />
        <activity
            android:name=".activities.A7_Actors"
            android:label="Actor" />
        <activity
            android:name=".activities.A8_Genres"
            android:label="Genre" />
        <activity android:name=".activities.AutocompleteActorAdapter" />
        <activity android:name=".activities.AutocompleteGenreAdapter" />
        <activity android:name=".activities.CustomDialog"></activity>
    </application>

